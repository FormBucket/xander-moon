{
  "name": "formbucket-ui",
  "version": "1.0.0",
  "description": "Powerful form handling for static sites",
  "author": "FunctionFoundry",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "clone:bare": "rm -rf ../formbucket-client.git; git clone --bare . ../formbucket-client.git",
    "clean": "rm -rf public",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:static": "node runMetalSmith && rm -rf public/img/ && cp -r img/ public/",
    "build": "npm run clean && npm run build:static && npm run build:webpack",
    "dev": "yarn start",
    "start": "yarn webpack-dev-server --public www-dev.formbucket.com --content-base dist/ --history-api-fallback",
    "start:static": "cd public && http-server . -p 3000",
    "lint": "eslint src",
    "deploy:www-next": "rsync -apvz -e \"ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null\" --progress ./public/ ops@10.8.0.10:/srv/www/formbucket.com/",
    "deploy:www-prod": "rsync -apvz -e \"ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null\" --progress ./public whqops@10.8.0.1:/srv/www/formbucket.com"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "formula": "^3.8.4",
    "highlight.js": "^9.6.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "preact": "^8.4.2",
    "preact-async-route": "^2.2.1",
    "preact-compat": "^3.18.5",
    "preact-router": "^2.6.1",
    "prismjs": "^1.16.0",
    "react": "^15.6.2 || ^16.0.0",
    "react-dnd": "^7.4.5",
    "react-fontawesome": "^1.1.0",
    "react-markdown": "^4.0.4",
    "react-monaco-editor": "^0.25.1",
    "react-simple-code-editor": "^0.9.10",
    "react-stripe-elements": "^2.0.1",
    "simplemde": "^1.11.2",
    "unistore": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server-spa": "^1.3.0",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "marked": {
    "breaks": true,
    "pedantic": false,
    "gfm": true,
    "tables": true,
    "sanitize": false,
    "smartLists": true,
    "smartypants": false,
    "xhtml": false
  }
}
